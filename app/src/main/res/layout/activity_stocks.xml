                android:backgroundTint="@color/darkpurple"
                android:text="item 10"
                android:textAlignment="viewStart"
                android:textSize="24sp" />

            <Button
                android:id="@+id/Sbutton20"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:backgroundTint="@color/darkpurple"
                android:text="item 11"
                android:textAlignment="viewStart"
                android:textSize="24sp" />

            <Button
                android:id="@+id/Sbutton19"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:backgroundTint="@color/darkpurple"
                android:text="item 12"
                android:textAlignment="viewStart"
                android:textSize="24sp" />

            <Button
                android:id="@+id/Sbutton18"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:backgroundTint="@color/darkpurple"
                android:text="item 13"
                android:textAlignment="viewStart"
                android:textSize="24sp" />

            <Button
                android:id="@+id/Sbutton14"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:backgroundTint="@color/darkpurple"
                android:text="item 14"
                android:textAlignment="viewStart"
                android:textSize="24sp" />

            <Button
                android:id="@+id/Sbutton15"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:backgroundTint="@color/darkpurple"
                android:text="item 15"
                android:textAlignment="viewStart"
                android:textSize="24sp" />

            <Button
                android:id="@+id/Sbutton16"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:backgroundTint="@color/darkpurple"
                android:text="item 16"
                android:textAlignment="viewStart"
                android:textSize="24sp" />

            <Button
                android:id="@+id/Sbutton17"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:backgroundTint="@color/darkpurple"
                android:text="item 17"
                android:textAlignment="viewStart"
                android:textSize="24sp" />
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>